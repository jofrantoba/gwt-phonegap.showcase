<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:mgwt="urn:import:com.googlecode.mgwt.ui.client.widget">
	
	<mgwt:panel.flex.RootFlexPanel>
	    <mgwt:header.HeaderPanel>
			<mgwt:button.image.PreviousitemImageButton ui:field="backButton" text="Modules"/>
			<mgwt:panel.flex.FlexSpacer/>
			<mgwt:header.HeaderTitle>Device</mgwt:header.HeaderTitle>
			<mgwt:panel.flex.FlexSpacer/>
			<mgwt:panel.flex.FixedSpacer/>
		</mgwt:header.HeaderPanel>
		<mgwt:panel.scroll.ScrollPanel scrollingEnabledX="false">
			<g:FlowPanel>
				<mgwt:list.widgetlist.WidgetList round="true">
					<g:HorizontalPanel>
						<g:HTML>Name: &nbsp;</g:HTML>
						<g:HTML ui:field="name"></g:HTML>
					</g:HorizontalPanel>
					<g:HorizontalPanel>
						<g:HTML>Platform: &nbsp;</g:HTML>
						<g:HTML ui:field="platform"></g:HTML>
					</g:HorizontalPanel>
					<g:HorizontalPanel>
						<g:HTML>Version: &nbsp;</g:HTML>
						<g:HTML ui:field="version"></g:HTML>
					</g:HorizontalPanel>
					<g:HorizontalPanel>
						<g:HTML>Phonegap Version: &nbsp;</g:HTML>
						<g:HTML ui:field="pversion"></g:HTML>
					</g:HorizontalPanel>
					<g:HorizontalPanel>
						<g:HTML>UUID: &nbsp;</g:HTML>
						<g:HTML ui:field="uuid"></g:HTML>
					</g:HorizontalPanel>
					<g:HorizontalPanel>
						<g:HTML>UUID: &nbsp;</g:HTML>
						<g:HTML ui:field="model"></g:HTML>
					</g:HorizontalPanel>
					<g:HorizontalPanel>
						<g:HTML>UUID: &nbsp;</g:HTML>
						<g:HTML ui:field="manufacturer"></g:HTML>
					</g:HorizontalPanel>
					<g:HorizontalPanel>
						<g:HTML>UUID: &nbsp;</g:HTML>
						<g:HTML ui:field="isVirtual"></g:HTML>
					</g:HorizontalPanel>
					<g:HorizontalPanel>
						<g:HTML>UUID: &nbsp;</g:HTML>
						<g:HTML ui:field="serial"></g:HTML>
					</g:HorizontalPanel>
				</mgwt:list.widgetlist.WidgetList>
			</g:FlowPanel>
		</mgwt:panel.scroll.ScrollPanel>
	</mgwt:panel.flex.RootFlexPanel>
</ui:UiBinder>
